{
  "manifestVersion": 1,
  "id": "vsts-debughelper-tasks",
  "version": "0.1.7",
  "name": "Build Debug Helpers",
  "scopes": [],
  "description": "A set of build/release tasks to help debug build/release pipelines.",
  "publisher": "knom",
  "galleryFlags": [
    "Preview"
  ],
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [{
    "id": "Microsoft.VisualStudio.Services"
  }],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "Debug",
    "Files",
    "Directory",
    "Directories",
    "Build",
    "Variables",
    "Environment Variables"
  ],
  "categories": [
    "Build and release"
  ],
  "screenshots": [{
    "path": "static/images/screen1.png"
  }],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://bit.ly"
    },
    "support": {
      "uri": "https://bit.ly"
    },
    "issues": {
      "uri": "https://bit.ly"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://bit.ly"
  },
  "branding": {
    "color": "rgb(250, 250,250)",
    "theme": "light"
  },
  "files": [{
      "path": "print-env-task"
    },
    {
      "path": "print-fs-structure-task"
    }
  ],
  "contributions": [{
      "id": "print-env-task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "print-env-task"
      }
    },
    {
      "id": "print-fs-structure-task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "print-fs-structure-task"
      }
    }
  ]
